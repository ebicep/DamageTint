{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "https://github.com/ebicep/DamageTintPlus",
    "sources": "https://github.com/ebicep/DamageTintPlus",
    "issues": "https://github.com/ebicep/DamageTintPlus/issues"
  },
  "license": "GPL-3.0",
  "icon": "icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.ebicep.damagetintplus.fabric.DamageTintPlusFabric"
      }
    ],
    "modmenu": [
      "com.ebicep.damagetintplus.config.fabric.ModMenuIntegration"
    ]
  },
  "mixins": [
    "damagetintplus.mixins.json",
    "damagetintplus-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${architectury_version}",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}",
    "cloth-config": ">=${cloth_config_version}"
  }
}
